<h2>住所情報登録</h2>

<%= form_for @address do |f| %>
  <%= render "devise/shared/error_messages", resource: @address %>

  <div class="field">
    <%= f.label :姓 %><br />
    <%= f.text_field :lastname %>
  </div>

  <div class="field">
    <%= f.label :名 %><br />
    <%= f.text_field :firstname %>
  </div>

  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
